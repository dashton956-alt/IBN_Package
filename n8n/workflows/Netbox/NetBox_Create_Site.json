    },
    {
      "parameters": {
        "jsCode": "return {\n  success: false,\n  action: 'already_exists',\n  message: `Site '${$('Prepare_Site_Data').item.json.site_name}' already exists`,\n  existing_site: $input.item.json.results[0]\n};"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        1120,
        440
