      "credentials": {
        "httpHeaderAuth": {
          "id": "zUcs6TpKWgVP6mF0",
          "name": "Header Auth account"
        }
      }
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "site-count-check",
              "leftValue": "={{ $json.count }}",
              "rightValue": 0,
              "operator": {
                "type": "number",
                "operation": "equals"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [
        900,
        340
      ],
      "id": "if-site-not-exists",
      "name": "If_Site_Not_Exists"
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://172.27.0.1:8443/api/dcim/sites/",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\n  \"name\": \"{{ $('Prepare_Site_Data').item.json.site_name }}\",\n  \"slug\": \"{{ $('Prepare_Site_Data').item.json.site_slug }}\",\n  \"status\": \"{{ $('Prepare_Site_Data').item.json.status || 'active' }}\"\n  {{ $('Prepare_Site_Data').item.json.facility ? ',\"facility\": \"' + $('Prepare_Site_Data').item.json.facility + '\"' : '' }}\n  {{ $('Prepare_Site_Data').item.json.description ? ',\"description\": \"' + $('Prepare_Site_Data').item.json.description + '\"' : '' }}\n}",
        "options": {}
      },
      "id": "create-site",
      "name": "Create_Site",
      "type": "n8n-nodes-base.httpRequest",
      "position": [
        1120,
        240
      ],
      "typeVersion": 4.2,
      "credentials": {
        "httpHeaderAuth": {
          "id": "zUcs6TpKWgVP6mF0",
          "name": "Header Auth account"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "return {\n  success: true,\n  action: 'created',\n  site: $input.item.json\n};"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        1340,
        240
      ],
      "id": "format-success-response",
