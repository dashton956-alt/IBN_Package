      "id": "trigger-create-site",
      "typeVersion": 1.1,
      "name": "When_Called_From_Another_Workflow",
      "type": "n8n-nodes-base.executeWorkflowTrigger",
      "position": [
        240,
        340
      ]
    },
    {
      "parameters": {
        "jsCode": "// Auto-generate slug if not provided\nconst siteName = $input.item.json.site_name;\nconst siteSlug = $input.item.json.site_slug || siteName.toLowerCase()\n  .replace(/[^a-z0-9]+/g, '-')\n  .replace(/^-|-$/g, '');\n\nreturn {\n  ...($input.item.json),\n  site_slug: siteSlug\n};"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        460,
        340
      ],
      "id": "prepare-site-data",
      "name": "Prepare_Site_Data"
    },
    {
      "parameters": {
        "url": "=http://172.27.0.1:8443/api/dcim/sites/?slug={{ encodeURIComponent($json.site_slug) }}",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "options": {}
      },
      "id": "check-site-exists",
      "name": "Check_Site_Exists",
      "type": "n8n-nodes-base.httpRequest",
      "position": [
        680,
